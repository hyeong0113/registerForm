    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity FamilyMember.
    -->
    <changeSet id="20190726232245-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="family_member_id"
                                 baseTableName="family_member_volunteer"
                                 constraintName="fk_family_member_volunteer_family_member_id"
